{
  "$schema": "https://raw.githubusercontent.com/nicegui-org/nicegui/main/.schemas/capabilities.schema.json",
  "identifier": "default",
  "description": "Default capabilities for DitLoop desktop",
  "windows": ["main", "workspace-*", "ditloop-*"],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "dialog:default",
    "notification:default",
    "os:default",
    "process:default",
    "updater:default",
    "deep-link:default"
  ]
}
